<%- | String $bibboxbaseurl | -%>

<VirtualHost *:80>
    ServerName activities.<%= $bibboxbaseurl %>
    <Proxy *>
        Order deny,allow
        Allow from all
    </Proxy>

    ProxyRequests           Off
    ProxyPreserveHost On
    ProxyPass               /       http://127.0.0.1:8050/
    ProxyPassReverse        /       http://127.0.0.1:8050/
</VirtualHost>